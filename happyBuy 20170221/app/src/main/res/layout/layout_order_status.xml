<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <LinearLayout
        android:id="@+id/layout_order_shipped"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:orientation="vertical">
        <RelativeLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                style="@style/award_cell_subject"
                android:layout_width="match_parent"
                android:text="@string/order_logistics_shipped"
                />
            <TextView
                android:id="@+id/order_shipped_time"
                style="@style/award_cell_time"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:text=""
                />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/grey_corner_fill_selector"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_web"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="left|center_vertical"
                android:textColor="@color/colorText"
                android:textSize="14sp"
                />
            <TextView
                android:id="@+id/tv_order_id"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="5dp"
                android:gravity="left|center_vertical"
                android:textColor="@color/colorText"
                android:textSize="14sp"
                />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:visibility="visible"
        android:id="@+id/layout_order_init"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/white_list_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_order_real_deliver"
            style="@style/award_cell_subject"
            android:layout_width="match_parent"
            android:text="@string/order_logistics_shippe"
            />
       <!-- android:textColor="@color/colorPrimary"-->

    </LinearLayout>
</FrameLayout>
